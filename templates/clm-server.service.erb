[Unit]
Description=Sonatype CLM Server
After=network.target

[Service]
EnvironmentFile=<%= @clm_environment_file %>
ExecStart=/usr/bin/java $JAVA_OPTIONS -jar $CLM_JAR server <%= @clm_config_file %>
User=<%= @clm_user %>
Group=<%= @clm_group %>

[Install]
WantedBy=multi-user.target
